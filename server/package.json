{
    "name": "server",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "aws-serverless-express": "^3.4.0",
        "claudia": "^5.14.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "debug": "^4.3.4",
        "dotenv-json": "^1.0.0",
        "express": "^4.18.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.7.3",
        "morgan": "^1.10.0",
        "node-cron": "^3.0.2",
        "pm2": "^5.2.2",
        "superagent": "^8.0.3",
        "superagent-throttle": "^1.0.1"
    },
    "scripts": {
        "start": "nodemon ./bin/www",
        "prod": "pm2 start ./bin/www",
        "init-claudia": "claudia generate-serverless-express-proxy --express-module server --set-env-from-json .env.json",
        "create-api": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-2 --set-env-from-json .env.json",
        "update-api": "claudia update --region us-east-2 --set-env-from-json .env.json"
    },
    "devDependencies": {
        "nodemon": "^2.0.20",
        "supertest": "^6.3.1"
    }
}